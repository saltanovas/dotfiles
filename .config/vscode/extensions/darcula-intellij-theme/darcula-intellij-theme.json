    "tokenColors": [
        // ========================================
        // === üì¶ STORAGE & TYPES ===
        // ========================================
        {
            "name": "Class/Type Names",
            "scope": [
                "support.type",
                "meta.return-type",
                "storage.type.cs",
                "storage.type.generic.cs",
                "storage.type.modifier.cs",
                "storage.type.variable.cs",
                "storage.type.groovy",
                "storage.type.annotation.groovy",
                "storage.type.parameters.groovy",
                "storage.type.generic.groovy",
                "storage.type.object.array.groovy",
                "storage.type.primitive.array.groovy",
                "storage.type.primitive.groovy"
            ],
            "settings": {
                "foreground": "#FFC66D"
            }
        },
        {
            "name": "Arrow Functions",
            "scope": "storage.type.function.arrow",
            "settings": {
                "foreground": "#A9B7C5"
            }
        },
        {
            "name": "Built-in Types (TS/JS specific)",
            "scope": [
                "meta.type.cast.expr",
                "meta.type.new.expr",
                "support.constant.math",
                "support.constant.dom",
                "support.constant.json"
            ],
            "settings": {
                "foreground": "#4EC9B0"
            }
        },

        // ========================================
        // === üè∑Ô∏è HTML/XML TAGS ===
        // ========================================
        {
            "name": "HTML/XML Tag Names",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#FFC66D"
            }
        },
        {
            "name": "HTML/XML Tag Brackets",
            "scope": "punctuation.definition.tag",
            "settings": {
                "foreground": "#FFC66D"
            }
        },
        {
            "name": "HTML/XML Attributes",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#BABABA"
            }
        },
        // ========================================
        // === üé® CSS/SCSS ===
        // ========================================
        {
            "name": "CSS Tag Selectors",
            "scope": ["entity.name.tag.css", "meta.selector"],
            "settings": {
                "foreground": "#d7ba7d"
            }
        },
        {
            "name": "CSS Classes, IDs, Pseudo-selectors",
            "scope": [
                "entity.other.attribute-name.class.css",
                "entity.other.attribute-name.class.mixin.css",
                "entity.other.attribute-name.id.css",
                "entity.other.attribute-name.parent-selector.css",
                "entity.other.attribute-name.pseudo-class.css",
                "entity.other.attribute-name.pseudo-element.css",
                "source.css.less entity.other.attribute-name.id",
                "entity.other.attribute-name.attribute.scss",
                "entity.other.attribute-name.scss"
            ],
            "settings": {
                "foreground": "#d7ba7d"
            }
        },
        {
            "name": "CSS Property Names",
            "scope": [
                "support.type.vendored.property-name",
                "support.type.property-name",
                "variable.css",
                "variable.scss",
                "variable.other.less"
            ],
            "settings": {
                "foreground": "#9876AA"
            }
        },
        {
            "name": "CSS Property Values",
            "scope": [
                "support.constant.property-value",
                "support.constant.font-name",
                "support.constant.media-type",
                "support.constant.media",
                "constant.other.color.rgb-value",
                "constant.other.rgb-value",
                "support.constant.color"
            ],
            "settings": {
                "foreground": "#CE9178"
            }
        },
        {
            "name": "CSS Units",
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#b5cea8"
            }
        },

        // ========================================
        // === üìù MARKDOWN ===
        // ========================================
        {
            "name": "Markdown Headings",
            "scope": [
                "entity.name.section.markdown",
                "markup.heading.setext.1.markdown",
                "markup.heading.setext.2.markdown",
                "markup.heading.setext.3.markdown",
                "markup.heading.setext.4.markdown",
                "markup.heading.setext.5.markdown",
                "markup.heading.setext.6.markdown"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#9876AA"
            }
        },
        {
            "name": "Markdown Punctuation",
            "scope": [
                "meta.separator.markdown",
                "punctuation.definition.list.begin.markdown",
                "punctuation.definition.bold.markdown",
                "punctuation.definition.heading.markdown",
                "punctuation.definition.italic.markdown",
                "punctuation.definition.table.markdown",
                "punctuation.separator.table.markdown",
                "punctuation.definition.constant.begin.markdown",
                "punctuation.definition.constant.end.markdown",
                "punctuation.definition.constant.markdown",
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#CC7832"
            }
        },
        {
            "name": "Markdown Links",
            "scope": [
                "meta.link.inet.markdown",
                "string.other.link.title.markdown",
                "string.other.link.description.markdown",
                "punctuation.definition.link.title.begin.markdown",
                "punctuation.definition.link.title.end.markdown",
                "punctuation.definition.link.description.begin.markdown",
                "punctuation.definition.link.description.end.markdown"
            ],
            "settings": {
                "fontStyle": "underline",
                "foreground": "#287BDE"
            }
        },
        {
            "name": "Markdown URLs",
            "scope": ["meta.link.reference.def.markdown", "meta.link.inline.markdown", "meta.image.inline.markdown"],
            "settings": {
                "foreground": "#FFC66D"
            }
        },
        {
            "name": "Markdown Code & Quotes",
            "scope": [
                "markup.quote.markdown",
                "markup.inline.raw.string.markdown",
                "markup.fenced_code.block.markdown",
                "markup.raw.block.markdown",
                "markup.inline.raw"
            ],
            "settings": {
                "foreground": "#6A8759"
            }
        },
        {
            "name": "Markdown Bold",
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#D0D4D8"
            }
        },
        {
            "name": "Markdown Italic",
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown Strikethrough",
            "scope": "markup.strikethrough.markdown",
            "settings": {
                "fontStyle": "strikethrough",
                "foreground": "#A9B7C5"
            }
        },
        {
            "name": "Markdown Inserted",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#b5cea8"
            }
        },
        {
            "name": "Markdown Deleted",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#ce9178"
            }
        },
        {
            "name": "Markdown Changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#A9B7C5"
            }
        },

        // ========================================
        // === üîß PREPROCESSOR & META ===
        // ========================================
        {
            "name": "Preprocessor Directives",
            "scope": "meta.preprocessor",
            "settings": {
                "foreground": "#CC7832"
            }
        },
        {
            "name": "Preprocessor Strings",
            "scope": "meta.preprocessor.string",
            "settings": {
                "foreground": "#ce9178"
            }
        },
        {
            "name": "Preprocessor Numbers",
            "scope": "meta.preprocessor.numeric",
            "settings": {
                "foreground": "#b5cea8"
            }
        },
        {
            "name": "Diff Headers",
            "scope": "meta.diff.header",
            "settings": {
                "foreground": "#CC7832"
            }
        },

        // ========================================
        // === üîå LANGUAGE SPECIFIC ===
        // ========================================
        {
            "name": "PHP Tags",
            "scope": ["punctuation.section.embedded.begin.metatag.php", "punctuation.section.embedded.end.metatag.php"],
            "settings": {
                "foreground": "#CC7832"
            }
        },
        {
            "name": "Git Rebase",
            "scope": "support.function.git-rebase",
            "settings": {
                "foreground": "#9876AA"
            }
        },
        {
            "name": "Git SHA",
            "scope": "constant.sha.git-rebase",
            "settings": {
                "foreground": "#b5cea8"
            }
        },

        // ========================================
        // === ‚ö†Ô∏è ERRORS & WARNINGS ===
        // ========================================
        {
            "name": "Invalid/Error",
            "scope": "invalid",
            "settings": {
                "foreground": "#f44747"
            }
        },
        {
            "name": "Info Token",
            "scope": "token.info-token",
            "settings": {
                "foreground": "#6796e6"
            }
        },
        {
            "name": "Warning Token",
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#cd9731"
            }
        },
        {
            "name": "Error Token",
            "scope": "token.error-token",
            "settings": {
                "foreground": "#f44747"
            }
        },
        {
            "name": "Debug Token",
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#b267e6"
            }
        },

        // ========================================
        // === üìö GENERAL MARKUP ===
        // ========================================
        {
            "name": "Emphasis",
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strong",
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Underline",
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "Header",
            "scope": "header",
            "settings": {
                "foreground": "#000080"
            }
        }
    ]
}
